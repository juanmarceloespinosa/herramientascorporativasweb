{
	"name": "herramientascorporativasweb",
	"version": "0.0.7",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"version-inc": "npm version patch --no-git-tag-version",
		"pm2": "pm2 start --name herramientas npm run start",
		"start": "PORT=3001 node build/index.js",
		"deploy-prod": "./deploy.sh .env.production herramientascorporativas"
	},
	"devDependencies": {
		"@mdi/js": "^7.4.47",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/nodemailer": "^6.4.14",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"less": "^4.2.0",
		"mysql2": "^3.9.3",
		"nodemailer": "^6.9.13",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"redis": "^4.6.13",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
